{
  "name": "recipe-sync",
  "version": "1.0.0",
  "description": "proof of concept realtime collaborative edit",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "server:debug": "nodemon --inspect src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:debug": "concurrently \"npm run server:debug\" \"npm run client\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback"
  },
  "author": "David Lai",
  "license": "ISC",
  "dependencies": {
    "@sibevin/random-token": "^1.0.2",
    "apollo-boost": "^0.3.1",
    "apollo-errors": "^1.9.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "formik": "^1.4.4",
    "google-libphonenumber": "^3.2.2",
    "graphql": "^14.3.0",
    "graphql-middleware": "^3.0.2",
    "graphql-tag": "^2.10.1",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.19",
    "luxon": "^1.14.0",
    "node-cache": "^4.2.0",
    "pg": "^7.11.0",
    "rate-limiter-flexible": "^1.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^5.0.0",
    "sqlite3": "^4.0.8",
    "unstated": "^2.1.1",
    "uuid": "^3.3.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "nodemon": "^1.17.3",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
